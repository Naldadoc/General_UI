        panel_color: 0.95,0.95,0.95,1
        MDBottomNavigationItem:
            name:'power_supply'
            text: 'Power Supply'
            icon: 'lightning-bolt'
            MDCard:
                orientation: 'vertical'
                elevation: 20
                pos_hint:{'center_x':0.5,'center_y':0.5}
