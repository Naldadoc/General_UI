                    width: 5
        MDBottomNavigationItem:
            name: 'DAQ'
            text:  'DAQ/Agilent'
            icon: 'monitor-eye'
        MDBottomNavigationItem:
            name: 'Wattmeter'
            text: 'Wattmeter'
            icon: 'chart-box-outline'
        MDBottomNavigationItem:
            name: 'chamber'
            text: 'Climatic Chamber'
            icon: 'thermometer'
        MDBottomNavigationItem:
            name: 'i/o_output'
            text: 'I/O management'
            icon: 'dip-switch'
        MDBottomNavigationItem:
            name: 'test_def'
            text: 'Test configuration'
            icon: 'tools'